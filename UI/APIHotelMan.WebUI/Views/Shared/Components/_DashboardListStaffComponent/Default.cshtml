@model List<ResultDashboardListStaffDto>
<div class="row">
    @foreach(var item in Model)
    {
        <div class="col-lg-3 col-sm-6">
            <div class="card">
                <div class="card-body">
                    <div class="text-center">
                        <img src="@item.ImageUrl" class="rounded-circle" alt="" style="width:100px; height:100px;">
                        <h5 class="mt-3 mb-1">@item.NameSurname</h5>
                        <p class="m-0">@item.Title</p>
                    </div>
                </div>
            </div>
        </div>
    }    
</div>
